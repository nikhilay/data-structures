package ctcLinkedList;
import java.util.*;

public class Palindrome {

	static public boolean check(Node firstnode){
		Node temp=firstnode;
		Node slow=firstnode;
		Node fast=firstnode;
		if(temp==null){
			System.out.println(" Empty List!! ");
			return false;
		}
		if(temp.next==null){
			System.out.println(" Only One Element !! ");
			return true;
		}
		Stack<Integer> stk= new Stack<Integer>();
		 
		while(fast!=null){
			stk.push(slow.data);
			slow=slow.next;
			fast=fast.next.next;
			if(fast.next==null){
				slow=slow.next;
				break;
			}
		  }
		while(!stk.isEmpty()){
		if(slow.data!=stk.pop()){
			return false;
		  } 
		}
		return true;
		
	}
	
}
