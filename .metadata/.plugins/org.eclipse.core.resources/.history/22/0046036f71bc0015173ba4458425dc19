import java.util.Scanner;


public class reverseStringStack {
	
	private int stacksize;
	private char[] stackstr;
	
	reverseStringStack(int size){
	
		stacksize=size;
		char[] stackstr= new char[stacksize];
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc= new Scanner(System.in);
		String inp=sc.nextLine();
		reverseStringStack input= new reverseStringStack(50);
		input.stackstr=inp.toCharArray();
		
		for(int i=0;i<input.stackstr.length;i++){
			System.out.print(input.stackstr[i]+" ");
		}
		
		

	}
	
	

}
