import java.io.*;

class GFG {
	public static void main (String[] args) {
		int n = 676;
		String ans = "";
		while(n>0){
    		int x = n%26;
    		
    		if(x==0){
    		    ans+=(String.valueOf((char)(96+26)));
    		    n=(n/26)-1;
    		} else {
    		    n=(n/26);
    		    ans+=(String.valueOf((char)(96+x)));
    		}
		}
		System.out.println(ans); // actual answer is reverse of 'ans'
	}
}